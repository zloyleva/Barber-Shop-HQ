<h2>Client page</h2>

<table class="table">
	<thead>
		<tr>
			<th>Name</th>
			<th>Phone</th>
			<th>Date and Time</th>
			<th>Barber</th>
			<th>Color</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td><%= @client.name %></td>
			<td><%= @client.phone %></td>
			<td><%= @client.datestamp %></td>
			<td><%= @client.barber %></td>
			<td><%= @client.color %></td>
		</tr>
	</tbody>
</table>
